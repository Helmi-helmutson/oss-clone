default	        menu.c32
KBDMAP		german.kbd
timeout		100
prompt		0
allowoptions	1
noescape	0
ui              gfxboot bootlogo

menu master passwd SHA1PASS
menu title OpenSchoolServer V3.4.2 PXE Boot Enviroment

LABEL Festplatte
    LOCALBOOT 0

LABEL CloneTool
        kernel clone/linux
        append initrd=clone/initrd install=exec:/bin/start_itool MemLimit=10 MemYASTText=10 Info=tftp://install/linuxrc.config ITOOL=partclone STARTCMD=clone SLEEP=1 apm=off noresume edd=off powersaved=off nohz=off highres=off processor.max_cstate=1 nomodeset x11failsafe


